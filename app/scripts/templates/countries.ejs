<div class="aggregate-bar">
  <h2 class="aggregate-title"><%= region %> Summary</h2>
</div>
<% _.each(aggregate.graphs, function(value, index) { %>
  <% if (value.mainValue !== '') { %>
  <div class="col-sm-4 col-md-4 col-xs-6 row-country">
	  <div class="inner">
		  <a class="more-info" href="#" data-toggle="popover" title="<%= value.name %>" data-content="<%= Mi.description[value.type] %>">?</a>
			  <span class="mi-ratio-value"><a><%= value.mainValue.toFixed(2) %>%</a></span>
				<span class="mi-ratio-label"><%= value.name %></span>
				<div id="chart-<%= value.type %>" class="hchart"></div>
				<span class="mi-crude-value"><%= numberWithCommas(value.crudeCoverage) %> lives insured</span>
				<span class="mi-year-value">Year: <%= value.year %></span>
			</div>
		</div>
  <% } %>
<% }) %>
<div class="aggregate-bar">
  <h2 class="aggregate-title">Countries</h2>
</div>
<% if (data.filter(function(d){ return d.mainValue !== ''; }).length) { %>
  <% _.each(data, function(value) { %>
    <% if (value.mainValue !== '') { %>
      <div class="col-sm-4 col-md-3 col-xs-6 row-country" data-mi-ratio="<%= value.mainValue %>">
        <div class="inner">
          <a class="more-info" data-toggle="popover" data-content="<%= Mi.description[type] %>">?</a>
          <b><%= value.country %></b>
          <br>
          <span class="mi-ratio-value"><a href="#country/<%= value.iso %>"><%= value.mainValue.toFixed(2) %>%</a></span>
          <span class="mi-ratio-label"><%= value.name %></span>
          <div id="<%= value.iso %>-chart" class="hchart"></div>
          <span class="mi-crude-value"><%= numberWithCommas(value.crudeCoverage) %> lives insured</span>
          <span class="mi-year-value">Year: <%= value.filterYear %></span>
        </div>
      </div>
    <% } %>
  <% }) %>
<% } else { %>
  <div class="aggregate-bar">
    <h2 class="aggregate-title">No data</h2>
  </div>
<% } %>
